{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/XiCore/(.*)", "dest": "/XiCore/$1" },
    { "src": "/music/(.*)", "dest": "/music/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800, immutable"
        }
      ]
    }
  ],
  "github": {
    "silent": true
  }
} 